{
  "name": "CoQuiz",
  "version": "2.9.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "postinstall": "react-native setup-ios-permissions && pod-install",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bump-patch": "npm version patch --no-git-tag-version && cd ios && bundle exec fastlane bump",
    "bump-minor": "npm version minor --no-git-tag-version && cd ios && bundle exec fastlane bump",
    "bump-major": "npm version major --no-git-tag-version && cd ios && bundle exec fastlane bump",
    "aab:android": "cd android && ./gradlew bundleRelease",
    "apk:android": "cd android && ./gradlew assembleRelease",
    "appcenter-android-key": "appcenter codepush deployment list -a coffeeking/coffeeking-android -k",
    "appcenter-ios-key": "appcenter codepush deployment list -a coffeeking/coffeeking-ios -k",
    "codepush:android": "appcenter codepush release-react -a coffeeking/coffeeking-android -d Production --sourcemap-output --output-dir ./build -m -t 1.0",
    "codepush:ios": "appcenter codepush release-react -a coffeeking/coffeeking-ios -d Production --sourcemap-output --output-dir ./build -m -t 1.0",
    "bundle:android": "react-native bundle --assets-dest build/CodePush --bundle-output build/CodePush/index.android.bundle --dev false --entry-file index.js --platform android --sourcemap-output build/CodePush/index.android.bundle.map",
    "bundle:ios": "react-native bundle --assets-dest build/CodePush --bundle-output build/CodePush/main.jsbundle --dev false --entry-file index.js --platform ios --sourcemap-output build/CodePush/main.jsbundle.map"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-firebase/analytics": "^16.7.0",
    "@react-native-firebase/app": "^16.7.0",
    "@react-native-firebase/messaging": "^16.7.0",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@web3modal/react-native": "^1.0.0-alpha.7",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "axios": "^1.4.0",
    "react": "18.1.0",
    "react-native": "^0.70.7",
    "react-native-code-push": "^7.1.0",
    "react-native-device-info": "^10.3.0",
    "react-native-exit-app": "^1.1.0",
    "react-native-get-random-values": "^1.9.0",
    "react-native-google-mobile-ads": "^13.3.1",
    "react-native-iap": "^12.15.1",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^3.8.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.31.1",
    "react-native-sound": "^0.11.2",
    "react-native-sound-player": "^0.13.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-webview": "^11.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "pod-install": "0.1.38",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "reactNativePermissionsIOS": [
    "AppTrackingTransparency",
    "Notifications"
  ]
}
